
<!-- Lauch the gazebo sim or connect to Sawyer-->
<!-- Launch Joint Action trajectory server -->
<!-- Launch Sawyer MoveIt config -->

<launch>

  <arg name='sim' default='False'/>

  <group if="$(arg sim)">
    <include file="$(find sawyer_gazebo)/launch/sawyer_world.launch">
      <arg name="electric_gripper" value="true"/>
    </include>
  </group>

  <node name="JointTrajectoryActionServer" pkg="intera_interface" type="joint_trajectory_action_server.py" output="screen"/>
  <node name="traj_with_sawyer" pkg="the_mighty_sawyer" type="traj_with_sawyer" output="screen"/>

</launch>